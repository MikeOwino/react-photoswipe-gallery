{
  "name": "react-photoswipe-gallery",
  "version": "0.0.5",
  "description": "PhotoSwipe wrapper for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "photoswipe",
    "react",
    "gallery"
  ],
  "repository": "https://github.com/irudoy/react-photoswipe-gallery.git",
